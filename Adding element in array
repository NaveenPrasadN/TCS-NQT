package Arrays;
import java.util.*;
public class AddingElementInArray {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int arr[]= {10,20,30};
		int newelement=100;
		int newarr[]=new int[arr.length+1];
		
		for(int i=0;i<arr.length;i++) {
			newarr[i]=arr[i];
		}
		newarr[arr.length]=newelement;
		System.out.println("Adding element in array:"+Arrays.toString(newarr));
	}

}
